[all:vars]
ansible_ssh_private_key_file=<pem_file_location>

[fluentd]  
fluentd          ansible_host=<broker_vm_ip>     ansible_connection=ssh  ansible_user=<broker_user>

[elasticsearch]
elasticsearch       ansible_host=<publisher_vm_ip>     ansible_connection=ssh  ansible_user=<publisher_user>

[subscriber]
subscriber      ansible_host=<subscriber_vm_ip>      ansible_connection=ssh  ansible_user=<subscriber_user>
